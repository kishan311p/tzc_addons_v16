    <!-- kits.b2b.pages -->
        <!-- home page main record -->
        <record id="kits_home_page" model="kits.b2b.pages">
            <field name="page_type">home</field>
            <field name="name">Home Page</field>
            <field name="header_text_1">Everything Optical – Optical wholesalers</field>
            <field name="text_1">Glasses suppliers since 1995, Everything Optical (ETO) are optical wholesalers distributing globally with a strong portfolio of branded designer glasses at exceptional prices. ETO offers amazing value with over 10,000 unique styles in designer sunglasses and eyeglasses. Now with an outstanding wholesaler web portal, you have access to all our designer frames at your fingertips. Register now and join our wholesale eyewear distribution network.
The team at ETO thank you for your business and remain committed everyday to stand with you as your best wholesale designer glasses source.
            </field>
            <field name="how_to_shop_title">How To Shop</field>
            <field name="how_to_shop_button_text">Shop Now</field>
            <field name="how_to_shop_background">https://cdn.teameto.com/data/B2C1/new_tzc_b2b_images/roadmap/roadmap-bg.png</field>
            <field name="how_to_shop_redirect_url">https://b2b.teameto.com/shop</field>
            <field name="text_1_redirect_url">https://b2b.teameto.com/aboutus</field>
            <field name="text_1_redirect_url_text">View Our Story</field>
            <field name="banner_url">https://cdn.teameto.com/data/B2C1/new_tzc_b2b_images/roadmap/roadmap-bg.png</field>
            <!-- <field name="banner_1_id" eval="kits_homepage_banner_image_1" /> -->
            <field name="website_id" eval="kits_team_eto_website" />
            <field name="how_to_shop_background">https://cdn.teameto.com/data/B2C1/new_tzc_b2b_images/roadmap/roadmap-bg.png</field>
            <field name="how_to_shop_redirect_url">https://b2b.teameto.com/shop</field>
            <field name="char_field">Some of Our Brands</field>
        </record>

