<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="kits_b2b_key_value_model_view_form" model="ir.ui.view">
        <field name="name">kits.b2b.key.value.model.view.form</field>
        <field name="model">kits.b2b.key.value.model</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="value" />
                        <field name="website_id" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <record id="kits_b2b_key_value_model_view_tree" model="ir.ui.view">
        <field name="name">kits.b2b.key.value.model.view.tree</field>
        <field name="model">kits.b2b.key.value.model</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name" />
                <field name="value" />
                <field name="website_id" />
            </tree>
        </field>
    </record>


    <record id="kits_b2b_key_value_model_action" model="ir.actions.act_window">
        <field name="name">Action Name</field>
        <field name="res_model">kits.b2b.key.value.model</field>
        <field name="view_mode">tree,form</field>
    </record>


</odoo>
