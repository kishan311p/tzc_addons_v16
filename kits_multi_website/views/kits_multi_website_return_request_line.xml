<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="kits_multi_website_return_request_line_view_form" model="ir.ui.view">
            <field name="name">kits.multi.website.return.request.line.view.form</field>
            <field name="model">kits.multi.website.return.request.line</field>
            <field name="arch" type="xml">
                <form>
                <header>
                <field name="state" />
                </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="quantity"/>
                                <field name="sale_order_line_id"/>
                                <field name="return_request_reason_id"/>
                            </group>
                            <field name="description" />
                            <group>
                                <field name="requested_date"/>
                                <field name="pickup_date"/>
                                <field name="received_date"/>
                                <field name="examined_date"/>
                                <field name="approved_date"/>
                                <field name="returned_date"/>
                                <field name="refunded_date"/>
                                <field name="scrapped_date"/>
                                <field name="rejected_date"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="kits_multi_website_return_request_line_view_tree" model="ir.ui.view">
            <field name="name">kits.multi.website.return.request.line.view.tree</field>
            <field name="model">kits.multi.website.return.request.line</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>