<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="kits_key_value_model_view_form" model="ir.ui.view">
        <field name="name">kits.key.value.model.view.form</field>
        <field name="model">kits.key.value.model</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="value" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="kits_key_value_model_view_tree" model="ir.ui.view">
        <field name="name">kits.key.value.model.view.tree</field>
        <field name="model">kits.key.value.model</field>
        <field name="arch" type="xml">
            <tree string="">
                <field name="name" />
                <field name="value" />
            </tree>
        </field>
    </record>

</odoo>
