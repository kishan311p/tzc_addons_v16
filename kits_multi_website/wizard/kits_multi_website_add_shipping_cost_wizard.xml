<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="add_shipping_cost_wizard_form_view" model="ir.ui.view">
            <field name="name">kits.multi.website.add.shipping.cost.wizard.form.view</field>
            <field name="model">kits.multi.website.add.shipping.cost.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="paid_shipping_cost_id"  options="{'no_create':True,'no_open':True}"/>
                                <field name="unit_price"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_precess" string="Process" type="object" class="oe_highlight" />
                        <button special="cancel" string="Cancel" type="object"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>

</odoo>
