<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="time_shift_view_form" model="ir.ui.view">
            <field name="name">time.shift.view.form</field>
            <field name="model">time.shift.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="estimate_date" />       
                        </group>
                    </sheet>
                    <footer>
                        <button string="Process" name="action_process" type="object" class="oe_highlight"/>
                        <button special="cancel" string="Cancel" />
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>