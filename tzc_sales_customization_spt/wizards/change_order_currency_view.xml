<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data noupdate="0">
        <record id="change_order_currency_view_form" model="ir.ui.view">
            <field name="name">change.order.currency.view.form</field>
            <field name="model">change.order.currency</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="currency_id" options="{'no_create':True,'no_open':True}"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="btn_process" type="object" string="Process" class="oe_highlight" />
                        <span style="margin-left:5px;margin-right:5px;">
                            OR
                        </span>
                        <button special="cancel" type="object" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>
</odoo>