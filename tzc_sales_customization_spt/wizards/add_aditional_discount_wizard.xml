<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="add_aditional_discount_wizard_form_view" model="ir.ui.view">
            <field name="name">add.aditional.discount.wizard.form.view</field>
            <field name="model">add.aditional.discount.wizard</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="discount_price"/>
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button name="action_add_discount_precess" string="Process" type="object" class="oe_highlight" />
                        <button special="cancel" string="Cancel" type="object"/>
                    </footer>
                </form>
            </field>
        </record>
    </data>

</odoo>