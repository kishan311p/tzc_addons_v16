<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="sale_order_report_wizard_from_view_spt" model="ir.ui.view">
            <field name="name">sale.order.report.wizard.from.view.spt</field>
            <field name="model">sale.order.report.wizard.spt</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name" />
                            </group>
                        </group>
                    </sheet>
                    <footer>
                        <button string="Download" class="btn btn-primary" type="object" name="action_process" />
                        <button string="Cancel" class="btn-secondary" special="cancel" />
                    </footer>
                </form>

            </field>
        </record>
    </data>
</odoo>
