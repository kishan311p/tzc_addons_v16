<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_config_settings_inherit_tzc_product_improt_spt" model="ir.ui.view">
        <field name="name">res.config.settings.inherit.tzc.product.improt.spt</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='product_setting_container'][2]" position="inside">
                <div class="col-12 col-lg-6 o_setting_box" id="sell_purchase_uom">
                    <div class="o_setting_left_pane">
                    </div>
                    <div class="o_setting_right_pane">
                        <label for="import_url"/>

                        <div class="text-muted">
                                Set product import instance url
                        </div>
                        <div class="content-group">
                            <div id="import_url"/>
                            <field name="import_url" />
                        </div>
                    </div>
                </div>
            </xpath>

        </field>
    </record>

</odoo>
