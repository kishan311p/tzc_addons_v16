<?xml version="1.0" encoding="utf-8"?>

<odoo noupdate="1">

    <record id="tzc_product_import_seq_spt" model="ir.sequence">
        <field name="name">Product Import Sequence</field>
        <field name="code">product.import.spt</field>
        <field name="prefix">IP/%(year)s/%(month)s/</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="product_import_on_barcode" model="ir.sequence">
        <field name="name">Product Import On Barcode Sequence</field>
        <field name="code">product.import.on.barcode</field>
        <field name="prefix">PI%(year)s%(month)s</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>
